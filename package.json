    "description": "Global modules and shared types",
    "devDependencies": {
        "@types/chai": "4.2.12",
        "@types/jsdom": "16.2.3",
        "@types/node": "14.0.27",
        "@typescript-eslint/eslint-plugin": "3.9.0",
        "@typescript-eslint/parser": "3.9.0",
        "chai": "4.2.0",
        "chalk": "4.1.0",
        "eslint": "7.7.0",
        "eslint-plugin-import": "2.22.0",
        "jsdom": "16.4.0",
        "mocha": "8.1.1",
        "nyc": "15.1.0",
