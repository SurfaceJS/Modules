{
    "author": "HitalloExiled",
    "description": "Global modules and shared types",
    "devDependencies": {
        "@types/chai": "4.2.11",
        "@types/commander": "2.12.2",
        "@types/glob": "7.1.2",
        "@types/minipass": "2.2.0",
        "@types/mocha": "7.0.2",
        "@types/node": "14.0.11",
        "chai": "4.2.0",
        "chalk": "4.0.0",
        "commander": "5.1.0",
        "glob": "7.1.6",
        "mocha": "7.2.0",
        "npm-registry-client": "8.6.0",
        "nyc": "15.1.0",
        "tar": "6.0.2",
        "tslib": "2.0.0"
    },
    "keywords": [
        "types",
        "tests",
        "shared"
