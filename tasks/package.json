        "task:relink": "node ./cli relink",
        "task:setup": "npm i && tsc && node ./cli setup",
        "task:unlink": "node ./cli unlink",
        "test": "mocha --ui tdd source/@surface/*/tests/*.spec.js"
    },
    "version": "1.0.0"
