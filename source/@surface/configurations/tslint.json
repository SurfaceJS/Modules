{    
    "rules":
    {
        "adjacent-overload-signatures":    true,
        "align":                           true,
        "await-promise":                   true,
        "binary-expression-operand-order": true,
        "class-name":                      true,
        "completed-docs":                  true,
        "curly":                           true,
        "cyclomatic-complexity":           true,
        "deprecation":                     true,
        "encoding":                        true,
        "indent":                          true,
        "interface-name":                  true,
        "import-spacing":                  true,
        "label-position":                  true,
        "member-access":                   true,
        "member-ordering":                 true,
        "new-parens":                      true,
        "no-angle-bracket-type-assertion": true,
        "no-any":                          true,
        "no-arg":                          true,
        "no-default-export":               true,
        "no-duplicate-imports":            true,
        "no-duplicate-super":              true,
        "no-duplicate-variable":           true,
        "no-magic-numbers":                true,
        "no-mergeable-namespace":          true,
        "no-reference":                    true,
        "prefer-for-of":                   true,
        "promise-function-async":          true,
        "restrict-plus-operands":          true,
        "semicolon":                       true,
        "switch-default":                  true,
        "typeof-compare":                  true,
        "use-isnan":                       true,
        "variable-name":                   true,
        "whitespace":                      true
    }
}