{
    "all": false,
    "branches": 90,
    "exclude": [
        "**/source/@surface/*/index.js",
        "**/tests"
    ],
    "extension": [
        ".js",
        ".ts"
    ],
    "functions": 90,
    "include": [
        "source/@surface/**/*.js",
        "source/@surface/**/*.ts"
    ],
    "lines": 90,
    "reporter": [
        "text-summary",
        "lcov"
    ],
    "sourceMap": true,
    "statements": 90
}